<Window x:Class="Mobile_Petitions.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mobile_Petitions"
        xmlns:viewmodels="clr-namespace:Mobile_Petitions.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" Height="650" Width="425" Closing="Window_Closing">
    <Window.DataContext>
        <viewmodels:MainViewModel/>
    </Window.DataContext>
    <Grid>
        <ContentControl Content="{Binding Viewmodels}" />
    </Grid>
    
</Window>
